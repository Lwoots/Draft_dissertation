	Journal = {Molecular biology and evolution},
	Language = {English},
	Month = {Jun},
	Number = {6},
	Pages = {1695-1701},
	Pmid = {22319168},
	Title = {PartitionFinder: Combined Selection of Partitioning Schemes and Substitution Models for Phylogenetic Analyses},
	Url = {https://www.ncbi.nlm.nih.gov/pubmed/22319168},
	Volume = {29},
