	Pages = {727-744},
	Pmid = {22605266},
	Title = {Anchored Hybrid Enrichment for Massively High-Throughput Phylogenomics},
	Url = {https://www.jstor.org/stable/41677974},
	Volume = {61},
	Year = {2012},
